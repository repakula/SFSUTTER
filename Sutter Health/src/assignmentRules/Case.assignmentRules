            <assignedTo>SHP_Member_A_G</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>AND(RecordType.Name = &apos;Grievance&apos;, 
   ISPICKVAL(Reason__c, &apos;Member Grievance&apos;), 
   ISPICKVAL(PRIORVALUE(Status) ,&apos;Resolved&apos;), 
   ISPICKVAL( PRIORVALUE(Secondary_Reason__c) ,&apos;Exempt&apos;) , 
   ISPICKVAL( Status , &apos;New&apos;), 
   OR( ISPICKVAL(Secondary_Reason__c ,&apos;Expedited&apos;), 
   ISPICKVAL( Secondary_Reason__c ,&apos;Routine&apos;) ) 
   )</formula>
