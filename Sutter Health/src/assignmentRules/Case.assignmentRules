            <formula>AND 
   (RecordType.Name = &apos;Grievance&apos;, 
   ISPICKVAL(Reason__c, &apos;Member Grievance&apos;), 
   ISPICKVAL( Secondary_Reason__c ,&apos;Expedited&apos;) || ISPICKVAL( Secondary_Reason__c ,&apos;Routine&apos;), 
   NOT(ISPICKVAL(Status,&quot;Closed&quot;)), 
   OR( 

   IF(ISCHANGED(Secondary_Reason__c), 
   NOT(OR 
   ( 
   ISPICKVAL(PRIORVALUE(Secondary_Reason__c),&apos;Routine&apos;), 
   ISPICKVAL(PRIORVALUE(Secondary_Reason__c),&apos;Expedited&apos;) 

   ) 
   ),ISNEW()) 
   ) 
   )</formula>
