        <ruleEntry>
            <formula>RecordType.Name = &apos;SHP Internal Service Request&apos;</formula>
        </ruleEntry>
        <ruleEntry>
            <formula>AND( RecordType.Name = &apos;SHP Chatter SR - Service&apos;, IsClosed )</formula>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
