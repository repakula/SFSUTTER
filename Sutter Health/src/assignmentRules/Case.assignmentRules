   (ISPICKVAL( Secondary_Reason__c ,&apos;Expedited&apos;) || ISPICKVAL( Secondary_Reason__c ,&apos;Routine&apos;)), 
   OR(ISCHANGED(Reason__c),ISNEW(),NOT(ISPICKVAL(Status,&quot;Closed&quot;))), 
   NOT(OR(IF(ISPICKVAL(PRIORVALUE(Secondary_Reason__c),&apos;Routine&apos;),true,false),IF(ISPICKVAL(PRIORVALUE(Secondary_Reason__c),&apos;Expedited&apos;),true,false) )),
   NOT(ISPICKVAL(Status,&quot;Closed&quot;)) 
   )</formula>
        </ruleEntry>
        <ruleEntry>
            <formula>AND( 
   RecordType.Name = &apos;Grievance&apos;, 
   ISPICKVAL(Reason__c, &apos;Member Grievance&apos;),
   NOT(ISNEW()), 
