        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
var url = parent.location.href;

var records = new sforce.SObject(&quot;Potential_Quality_Issue__c&quot;); //fetch the Record
records = &apos;{!Potential_Quality_Issue__c.Id}&apos;;  // Fetch the RecordId
var Status = &apos;{!Potential_Quality_Issue__c.PQI_Status__c}&apos;;  //Fetch the Status
var Owner= &apos;{!Potential_Quality_Issue__c.OwnerId}&apos;;  // Fetch the owner of record 
Owner=Owner.substring(0, 15);                    // Convert the Owner ID in 15 digit  
var currentUser= sforce.connection.getUserInfo().userId;
currentUser= currentUser.substring(0, 15);  //convert the currentuser id in 15 digits
var updateRecords = [];

var update_PQI = new sforce.SObject(&quot;Potential_Quality_Issue__c&quot;);
update_PQI.Id = records;

if(Owner == currentUser &amp;&amp; Status!= &quot;Closed&quot;)
{ 
if(Status == &quot;PRC&quot; || Status == &quot;UM Review&quot; ){
var result = sforce.connection.query(&quot;Select Id, SobjectType, QueueId, Queue.Name From QueueSobject  where Queue.Name = &apos;UM Reviewers&apos;&quot;);
var queue_records = result[&apos;records&apos;];	
update_PQI.OwnerId = queue_records[&apos;QueueId&apos;];
updateRecords.push(update_PQI );
}
else if(Status == &quot;MD Review&quot;){
var result = sforce.connection.query(&quot;Select Id, SobjectType, QueueId, Queue.Name From QueueSobject  where Queue.Name = &apos;CMO&apos;&quot;);
var queue_records = result[&apos;records&apos;];
update_PQI.OwnerId = queue_records[&apos;QueueId&apos;];
updateRecords.push(update_PQI );
}
else{
var result = sforce.connection.query(&quot;Select Id, SobjectType, QueueId, Queue.Name From QueueSobject  where Queue.Name = &apos;Quality&apos;&quot;);
var queue_records = result[&apos;records&apos;];
update_PQI.OwnerId = queue_records[&apos;QueueId&apos;];
updateRecords.push(update_PQI );
}
}
else{
alert(&apos;You are not the Owner of this PQI&apos;);
}
result = sforce.connection.update(updateRecords);
parent.location.href = url;</url>
