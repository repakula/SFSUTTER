            <script> 
            $j = jQuery.noConflict();
            var tempValuePhone = '-1';
            var tempValueFax = '-1';
            function removeMandatory(element,type) {
                if(tempValuePhone == '-1') {
                    tempValuePhone = document.getElementById(element).value;
                }
                if(type == 'Phone') {
                    if($j("[id$=Phonecheckbox]").is(':checked')){
                        if($j('#ReqPhone').hasClass('requiredBlock')) {
                            $j('#ReqPhone').removeClass('requiredBlock');
                        }
                        document.getElementById(element).readOnly = true;
                        document.getElementById(element).value = '';
                    }else {
                        $j('#ReqPhone').addClass('requiredBlock');
                        $j('[id$=ReqPhone]').css("display", "block");
                        document.getElementById(element).readOnly = false;
                        document.getElementById(element).value = tempValuePhone;
                    }
                }
            }
            
            function removeMandatory1(element,type) {
                if(tempValueFax == '-1') {
                    tempValueFax = document.getElementById(element).value;
                }
                if(type == 'Fax') {
                    // var tempValue = document.getElementById(element).value;
                    if($j("[id$=FaxCheckbox]").is(':checked')){
                        if($j('#ReqFax').hasClass('requiredBlock')) {
                            $j('#ReqFax').removeClass('requiredBlock');
                        }
                        document.getElementById(element).readOnly = true;
                        document.getElementById(element).value = '';
                        
                    }else{
                        $j('#ReqFax').addClass('requiredBlock');
                        $j('[id$=ReqFax]').css("display", "block");
                        document.getElementById(element).readOnly = false;
                        document.getElementById(element).value = tempValueFax;
                    }
                    
                }  
            }
            function cancelOnComp() {
                var validateFlag = $j('input[id$=CancelCase]').val();
                if(validateFlag == 'true') {
                    var ConfirmFlag = confirm('Are you sure you want to cancel the case ?');
                    if(validateFlag == 'true' && ConfirmFlag) {
                        closeTab();
                    }
                }
            }
            
            function backToProvider() {
                closeTab();
            }
            var isRightDivVisible;
            function showRightDiv() {
                $j('input[id$=contactProviderbtn]').css("display", "none");
                $j('#LeftContent').removeClass('leftwidthFfull');
                $j('#LeftContent').addClass('leftwidth');
                $j('#RightContent').removeClass('rightContentDisplayHide');
                $j('#RightContent').addClass('rightContentDisplay');
                isRightDivVisible = 'true';
                $j('.validateBtnDisplayHide').removeClass('validateBtnDisplayHide');
                $j('#ValidateAddress').addClass('validateBtnDisplay');
                $j('.pbSubsection1').addClass('pbSubsection1');
                createCaseAF();
                $j('.pbSubsection1').addClass('pbSubsection1');
                disableaddressFields();
            }
            
            //REQ 3665
            function reValidateFunc() {
                $j('input[id$=contactProviderbtn]').css("display", "none");
                $j('#LeftContent').removeClass('leftwidthFfull');
                $j('#LeftContent').addClass('leftwidth');
                $j('#RightContent').removeClass('rightContentDisplayHide');
                $j('#RightContent').addClass('rightContentDisplay');
                isRightDivVisible = 'true';
                $j('.validateBtnDisplayHide').removeClass('validateBtnDisplayHide');
                $j('#ValidateAddress').addClass('validateBtnDisplay');
                $j('.pbSubsection1').addClass('pbSubsection1');
                var reValidateCase = true;
                createCaseReValidateAF(reValidateCase);
                $j('.pbSubsection1').addClass('pbSubsection1');
                disableaddressFields();
                if($j("[id$=resigned]").is(':checked')){
                    showresignedProviderRefer(true);
                }
                
            }
            function disableaddressFields(){
                var caseStatus = $j('input[id$=CaseStatus]').val();
                if(isRightDivVisible == 'true' && caseStatus != 'Closed') {
                    $j('#providerDetails').prop('disabled',false);
                    if($j("[id$=resigned]").is(':checked')){
                        showresignedProviderRefer(true);
                    }
                }else{
                    $j('#providerDetails').prop('disabled',true);
                    if($j("[id$=resigned]").is(':checked')){
                        showresignedProviderRefer(true);
                    }
                }
            }
            
            function saveaddress() {
                SaveUpdatedAddress();
            }
            $j(document).ready(function() {
                $j('[id$=ReqEmail]').removeClass('requiredBlock');
                 //Req: 3909 - Handling required for fax and email based on communication preference values.
                if('{!isEmailTrue}' == 'true') {
                   $j('[id$=ReqEmail]').addClass('requiredBlock');
                    $j('[id$=ReqEmail]').css("display", "block");
                }
                else
                {
                   $j('[id$=ReqEmail]').removeClass('requiredBlock');
                }
                                
                if('{!isCreatingCase}' == 'true') {
                    $j('input[id$=contactProviderbtn]').css("display", "none");
                    $j('#LeftContent').removeClass('leftwidthFfull');
                    $j('#LeftContent').addClass('leftwidth');
                    $j('#RightContent').removeClass('rightContentDisplayHide');
                    $j('#RightContent').addClass('rightContentDisplay');
                    isRightDivVisible = 'true';
                    $j('.validateBtnDisplayHide').removeClass('validateBtnDisplayHide');
                    $j('#ValidateAddress').addClass('validateBtnDisplay');
                    disableaddressFields();
                }else{
                    $j('#providerDetails').prop('disabled',true);
                    if($j("[id$=resigned]").is(':checked')){
                        showresignedProviderRefer(true);
                    }
                }
                
                if($j("[id$=retired]").is(':checked') && '{!isCreatingCase}' == 'true'){
                    disableText(true);
                }
                
                if($j("[id$=resigned]").is(':checked') == false){
                    document.getElementById('resignedReferDiv').style.display = 'none';
                }
                
            });
            
            function retiredFunction(selectedValue){
                if(selectedValue == true){
                    disableText(true);
                    $j("[id$=practicingPhysician]").prop( "checked", false );
                    $j("[id$=resigned]").prop( "checked", false );
                    $j("[id$=resignedProvider_refer]").prop( "checked", false );
                    document.getElementById('resignedReferDiv').style.display = 'none';
                }else{
                    disableText(false);
                }
            }
            
            //PDI - Wave1 Code Change
            function practicingdisableText(selectedValue) 
            {
                if(selectedValue == true){
                    $j("[id$=resigned]").prop( "checked", false );
                    $j("[id$=retired]").prop( "checked", false );
                    $j("[id$=resignedProvider_refer]").prop( "checked", false );
                    document.getElementById('resignedReferDiv').style.display = 'none';
                    disableText(false);
                }
                
            }
            function disableText(selectedValue) 
            {
                if( selectedValue == true)
                {
                    $j("[id$=primaryEditBtn]").prop("disabled",true);
                    $j("[id$=secEditBtn]").prop("disabled",true);
                    $j("[id$=secDeleteBtn]").prop("disabled",true);
                    $j("[id$=newAddrBtn]").prop("disabled",true);
                    $j("[id$=reorderBtn]").prop("disabled",true);
                    $j("[id$=convertBtn]").prop("disabled",true);
                    if('{!providerSearchFlag}' == 'true'){ 
                        $j("[id$=speciality]").prop("disabled",true);
                        $j("[id$=department]").prop("disabled",true);
                        $j("[id$=hospitalAff]").prop("disabled",true);
                        $j("[id$=medicalAff]").prop("disabled",true);
                        $j("[id$=medicalEducation]").prop("disabled",true);
                        $j("[id$=webAddress]").prop("disabled",true);
                        $j("[id$=internship]").prop("disabled",true);
                        $j("[id$=professionalInterest]").prop("disabled",true);
                        $j("[id$=residency]").prop("disabled",true);
                        $j("[id$=onlineServices]").prop("disabled",true);
                        $j("[id$=lanaguages]").prop("disabled",true);
                        $j("[id$=certifications]").prop("disabled",true);
                        $j("[id$=commActivities]").prop("disabled",true);
                        $j("[id$=publications]").prop("disabled",true);
                        $j("[id$=persInterests]").prop("disabled",true);
                        $j("[id$=additionalInfo]").prop("disabled",true);
                        $j("[id$=philosophyCare]").prop("disabled",true);
                        $j("[id$=adminAssistantName]").prop("disabled",true);
                        $j("[id$=assistantPhone]").prop("disabled",true);
                        $j("[id$=assistantEmail]").prop("disabled",true);
                        $j("[id$=adminAsstTitle]").prop("disabled",true);
                        $j("[id$=delPhoneExt]").prop("disabled",true);
                        $j("[id$=nameChageSuggested]").prop("disabled",true);
                    }else{
                        $j("[id$=delegateName]").prop("disabled",true);
                        $j("[id$=delegatePhone]").prop("disabled",true);
                        $j("[id$=delegateEmail]").prop("disabled",true);
                        $j("[id$=delegateTitle]").prop("disabled",true);
                        $j("[id$=delegatePhoneExt]").prop("disabled",true);
                        $j("[id$=nameChagesSuggested]").prop("disabled",true);
                    }
                }else if(selectedValue == false){
                    $j("[id$=primaryEditBtn]").prop("disabled",false);
                    $j("[id$=secEditBtn]").prop("disabled",false);
                    $j("[id$=secDeleteBtn]").prop("disabled",false);
                    $j("[id$=newAddrBtn]").prop("disabled",false);
                    $j("[id$=reorderBtn]").prop("disabled",false);
                    $j("[id$=convertBtn]").prop("disabled",false);
                    
                    if('{!providerSearchFlag}' == 'true'){
                        $j("[id$=speciality]").prop("disabled",false);
                        $j("[id$=department]").prop("disabled",false);
                        $j("[id$=hospitalAff]").prop("disabled",false);
                        $j("[id$=medicalAff]").prop("disabled",false);
                        $j("[id$=medicalEducation]").prop("disabled",false);
                        $j("[id$=webAddress]").prop("disabled",false);
                        $j("[id$=internship]").prop("disabled",false);
                        $j("[id$=professionalInterest]").prop("disabled",false);
                        $j("[id$=residency]").prop("disabled",false);
                        $j("[id$=onlineServices]").prop("disabled",false);
                        $j("[id$=lanaguages]").prop("disabled",false);
                        $j("[id$=certifications]").prop("disabled",false);
                        $j("[id$=commActivities]").prop("disabled",false);
                        $j("[id$=publications]").prop("disabled",false);
                        $j("[id$=persInterests]").prop("disabled",false);
                        $j("[id$=additionalInfo]").prop("disabled",false);
                        $j("[id$=philosophyCare]").prop("disabled",false);
                        $j("[id$=adminAssistantName]").prop("disabled",false);
                        $j("[id$=assistantPhone]").prop("disabled",false);
                        $j("[id$=assistantEmail]").prop("disabled",false);
                        $j("[id$=adminAsstTitle]").prop("disabled",false);
                        $j("[id$=delPhoneExt]").prop("disabled",false);
                        $j("[id$=nameChageSuggested]").prop("disabled",false);    
                    }else{
                        $j("[id$=delegateName]").prop("disabled",false);
                        $j("[id$=delegatePhone]").prop("disabled",false);
                        $j("[id$=delegateEmail]").prop("disabled",false);
                        $j("[id$=delegateTitle]").prop("disabled",false);
                        $j("[id$=delegatePhoneExt]").prop("disabled",false);
                        $j("[id$=nameChagesSuggested]").prop("disabled",false);
                    }
                }
            }
            
            function showresignedProviderRefer(status){
                if(status == true){
                    var caseStatus = $j('input[id$=CaseStatus]').val();
                    $j("[id$=practicingPhysician]").prop( "checked", false );
                    $j("[id$=retired]").prop( "checked", false );
                    document.getElementById('resignedReferDiv').style.display = 'block';
                    if(caseStatus != 'Closed' && '{!isCreatingCase}' == 'true'){
                        if($j("[id$=resignedProvider_refer]").is(':checked')){
                            referreddisableText(true);
                        }else{
                            referreddisableText(false);
                        }
                    }else{
                        referreddisableText(false);
                    }
                }else{
                    disableText(false);
                    $j("[id$=resignedProvider_refer]").prop( "checked", false );
                    document.getElementById('resignedReferDiv').style.display = 'none';
                }
                
            }
            
            function referreddisableText(status){
                if(status == false){
                    disableText(true);
                    
                }else{
                    disableText(false);
                    
                }
            }
            
            function formatPhone(obj,value1) {
                var numbers = value1.replace(/\D/g, ''),
                    char = {0:'(',3:') ',6:'-'};
                var tempVal = '';
                for (var i = 0; i < numbers.length; i++) {
                    tempVal += (char[i]||'') + numbers[i];
                }
                document.getElementById(obj).value = tempVal;
            }

            function loading(val) {
                if (val) {
                    document.getElementById('contentLoading').style.display = 'block';
                }
                else {
                    document.getElementById('contentLoading').style.display = 'none';
                }
            }
            
            var closeSubtab = function closeSubtab(result) {
                //Now that we have the tab ID, we can close it
                var tabId = result.id;
                sforce.console.refreshNavigationTab();
                sforce.console.closeTab(tabId);
            }
            
            function closeTab() {
                sforce.console.getEnclosingTabId(closeSubtab);
            }
            
            function acceptPDV() {
                //Req-3940: If agent clicks on yes button we are passing a value to controller that agent wants to accept record.
                document.getElementById('{!$Component.frm.acceptPDIRecord}').value = true;
                acceptPDVAF();
            }
            function mapSuggestedAddress(input){
                var selectedVal = input.value;
                document.getElementById('{!$Component.frm.RadioButtonValue}').value = selectedVal;
                saveSuggestedAddress();
            }
            function disableAddressFields(value){
                if(value == true){
                    $j("[id$=editAddrLine1]").prop("disabled",true);
                    $j("[id$=editAddrLine2]").prop("disabled",true);
                    $j("[id$=editAddrCity]").prop("disabled",true);
                    $j("[id$=editAddrState]").prop("disabled",true);
                    $j("[id$=editAddrZipCode]").prop("disabled",true);
                     $j("[id$=suggestedSaveAddressBtn]").prop("disabled",true);
                    
                }else{
                    $j("[id$=editAddrLine1]").prop("disabled",false);
                    $j("[id$=editAddrLine2]").prop("disabled",false);
                    $j("[id$=editAddrCity]").prop("disabled",false);
                    $j("[id$=editAddrState]").prop("disabled",false);
                    $j("[id$=editAddrZipCode]").prop("disabled",false);
                    $j("[id$=suggestedSaveAddressBtn]").prop("disabled",false);
                    $j("[id$=addressWarningMessage]").hide();
                    
                }
            }
            function disableAddresses(value){
                if(value == true){
                    $j("[id$=editAddrLine1]").prop("disabled",true);
                    $j("[id$=editAddrLine2]").prop("disabled",true);
                    $j("[id$=editAddrCity]").prop("disabled",true);
                    $j("[id$=editAddrState]").prop("disabled",true);
                    $j("[id$=editAddrZipCode]").prop("disabled",true);
                }else{
                    $j("[id$=editAddrLine1]").prop("disabled",false);
                    $j("[id$=editAddrLine2]").prop("disabled",false);
                    $j("[id$=editAddrCity]").prop("disabled",false);
                    $j("[id$=editAddrState]").prop("disabled",false);
                    $j("[id$=editAddrZipCode]").prop("disabled",false);
                }
            }
            </script>
