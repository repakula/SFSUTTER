     <!-- Req-3357 JAVASCRIPT for show and Hide IL, Article,Contact Detail  -->
    <script>
       
         function setVisibility(elemtId){
            var SearchDiv = "SearchDiv";
            var Interactionlog = "Interactionlog";
            var Article = "Article";
            var clientdetail = document.getElementById('SearchDiv');
            var Interactionlog = document.getElementById('InteractionLog');
            var Article = document.getElementById('Article');
            if(elemtId == 'SearchDiv' ){
                clientdetail.style.display = 'block';   
                Article.style.display = 'none';
                Interactionlog.style.display ='none';
                document.getElementById('ClientDetailTab').style.backgroundColor='#1797C0';
                document.getElementById('InteractionLogTab').style.backgroundColor= '#49B7DA';
                document.getElementById('ArticleTab').style.backgroundColor= '#49B7DA';
            }
            if(elemtId == 'Article' ){
                clientdetail.style.display = 'none';    
                Article.style.display = 'block';
                Interactionlog.style.display ='none';
                document.getElementById('ClientDetailTab').style.backgroundColor= '#49B7DA';
                document.getElementById('InteractionLogTab').style.backgroundColor= '#49B7DA';
                document.getElementById('ArticleTab').style.backgroundColor='#1797C0';
            }
            if(elemtId == 'Interactionlog' ){
                clientdetail.style.display = 'none';    
                Article.style.display = 'none';
                Interactionlog.style.display = 'block';
                document.getElementById('ClientDetailTab').style.backgroundColor= '#49B7DA';
                document.getElementById('InteractionLogTab').style.backgroundColor='#1797C0';
                document.getElementById('ArticleTab').style.backgroundColor= '#49B7DA';
            }
        }
        
    </script>
