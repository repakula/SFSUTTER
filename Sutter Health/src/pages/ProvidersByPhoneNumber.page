                }
                function formatPhone(obj,value1) {
                    var numbers = value1.replace(/\D/g, ''),
                        char = {0:'(',3:') ',6:'-'};
                    var tempVal = '';
                    for (var i = 0; i < numbers.length; i++) {
                        tempVal += (char[i]||'') + numbers[i];
                    }
                    document.getElementById(obj).value = tempVal;
                }
                </script>
