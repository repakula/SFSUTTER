    <script language="javascript">
      
        function setHDHP(hdhp,hdhp1){
            var tempVar  = document.getElementById(hdhp).value; 
                       
            if (tempVar.indexOf('HS')> -1 || tempVar.indexOf('HL')> -1){
                hdhp1 = hdhp1 + ':0';
                document.getElementById(hdhp1).checked = true;
            } else {
                hdhp1 = hdhp1 + ':1';
                document.getElementById(hdhp1).checked = true;
            }          
        }
        
        /* on selection on ERISA field if yes then show the other not editable or if no the show field as editable*/
        function setShowHideOtheERISA(paramId){
           var radioValue;   
           if(document.getElementById(paramId).checked) {
                radioValue = document.getElementById(paramId).value;
                if(radioValue == 'True') {
                    document.getElementById('inptTextField').style.display = 'block';
                    document.getElementById('inptField').style.display = 'none';
                } else {
                    document.getElementById('inptTextField').style.display = 'none';
                    document.getElementById('inptField').style.display = 'block';
                }
            }
        }
    </script>
    
