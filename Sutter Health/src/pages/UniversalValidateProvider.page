                                                                    </div>                              
                                                                </div>   
                                                            </div><!--Retired Div Ends -->
                                                        </div>
                                                    </div><!-- Provider Status Panel Ends -->
                                                    <apex:outputText rendered="{!if(providerSearchFlag == false, true, false)}">
