    function practicingFuncText(selectedValue) 
    {
        if(selectedValue == true){
            document.getElementById('{!$Component.theForm.practicing}').checked = true;
            document.getElementById("btnradio2").checked = false;
            document.getElementById("btnradio3").checked = false;
            //disableText(false);
            resignedFuncText(false);
            retiredFuncText(false);
            if(document.documentElement.clientWidth < 600){
                document.getElementById("practiceHelp").style.display = 'block';
                document.getElementById("resignHelp").style.display = 'none';
                document.getElementById("retiredHelp").style.display = 'none';
            }
        }else{
            document.getElementById('{!$Component.theForm.practicing}').checked = false;
        }
    }
    
    function resignedFuncText(selectedValue) 
    {
        if(selectedValue == true){
            document.getElementById('{!$Component.theForm.resigned}').checked = true;
            document.getElementById('{!$Component.theForm.practicing}').checked = false;
            document.getElementById('{!$Component.theForm.retired}').checked = false;
            document.getElementById("btnradio1").checked = false;
            document.getElementById("btnradio3").checked = false;
            //Displaying Resigned Refer Div, if resigned is checked
            document.getElementById("resignedReferDiv").style.display = 'block';
            resignreferreddisableText(false);
            //To display Help text in mobile view
            if(document.documentElement.clientWidth < 600){
                document.getElementById("resignHelp").style.display = 'block';
                document.getElementById("practiceHelp").style.display = 'none';
                document.getElementById("retiredHelp").style.display = 'none';
            }
        }else{
            document.getElementById('{!$Component.theForm.resigned}').checked = false;
            document.getElementById("resignedReferDiv").style.display = 'none';
            document.getElementById('{!$Component.theForm.resignedProvider_refer}').checked = false;
            disableText(false);
        }
    }
    
    function retiredFuncText(selectedValue) 
    {
        if(selectedValue == true){
            document.getElementById('{!$Component.theForm.retired}').checked = true;
            document.getElementById('{!$Component.theForm.practicing}').checked = false;
            document.getElementById('{!$Component.theForm.resigned}').checked = false;
            document.getElementById('{!$Component.theForm.resignedProvider_refer}').checked = false;
            document.getElementById("btnradio1").checked = false;
            document.getElementById("btnradio2").checked = false;
            disableText(true);
            document.getElementById("resignedReferDiv").style.display = 'none';
            $("#btnCheckbox4").prop('checked', false);
            //To display Help text in mobile view
            if(document.documentElement.clientWidth < 600){
                document.getElementById("retiredHelp").style.display = 'block';
                document.getElementById("practiceHelp").style.display = 'none';
                document.getElementById("resignHelp").style.display = 'none';
            }
        }else{
            document.getElementById('{!$Component.theForm.retired}').checked = false;
            disableText(false);
        }
    }
    
    function resignreferreddisableText(statusVal){
        if(statusVal == true){
            disableText(false);
            document.getElementById('{!$Component.theForm.resignedProvider_refer}').checked = true;
            $("#resignProviderreferdialog").dialog("open");
        }else{
            $("#btnCheckbox4").prop('checked', false);
            disableText(true);
        }
    }
    
    function disableText(selectedValue) 
    {   
        if( selectedValue == true)
        {
            var profileId = '{!userObj.profileId}';
            if(profileId != '{!$Label.CL578_PDI_AdminAsstProfileId}'){
                if({!providerSearchFlag == false}){
                    document.getElementById('{!$Component.theForm.delgateName}').disabled = true;
                    document.getElementById('{!$Component.theForm.delegateEmail}').disabled = true;
                    document.getElementById('{!$Component.theForm.delgatephone}').disabled = true;
                    document.getElementById('{!$Component.theForm.delegatetitle}').disabled = true;
                }else{
                    document.getElementById('{!$Component.theForm.adminAssistant}').disabled = true;
                    document.getElementById('{!$Component.theForm.assistantEmail}').disabled = true;
                    document.getElementById('{!$Component.theForm.adminAssistantphone}').disabled = true;
                    document.getElementById('{!$Component.theForm.adminAssistanttitle}').disabled = true;
                }
            }
            $("#primaryEditBtn").attr("disabled", true);
            $("[id*=secondaryEditBtn]").attr("disabled", true);
            $("[id*=secondaryDeleteBtn]").attr("disabled", true);
            $("[id*=convertBtn]").attr("disabled", true);
            $("#newAddressBtn").attr("disabled", true);
            $("[id$=reorderBtn]").hide();
            
        }else if(selectedValue == false)
        {
            var profileId = '{!userObj.profileId}';
            if(profileId != '{!$Label.CL578_PDI_AdminAsstProfileId}'){
                if({!providerSearchFlag == false}){
                    document.getElementById('{!$Component.theForm.delgateName}').disabled = false;
                    document.getElementById('{!$Component.theForm.delegateEmail}').disabled = false;
                    document.getElementById('{!$Component.theForm.delgatephone}').disabled = false;
                    document.getElementById('{!$Component.theForm.delegatetitle}').disabled = false;
                }else{
                    document.getElementById('{!$Component.theForm.adminAssistant}').disabled = false;
                    document.getElementById('{!$Component.theForm.assistantEmail}').disabled = false;
                    document.getElementById('{!$Component.theForm.adminAssistantphone}').disabled = false;
                    document.getElementById('{!$Component.theForm.adminAssistanttitle}').disabled = false;
                }
            }
            $("#primaryEditBtn").attr("disabled", false);
            $("[id*=secondaryEditBtn]").attr("disabled", false);
            $("[id*=secondaryDeleteBtn]").attr("disabled", false);
            $("[id*=convertBtn]").attr("disabled", false);
            $("#newAddressBtn").attr("disabled", false);
            $("[id$=reorderBtn]").show();
        }
    }
    function validateProvider(){
        if(document.getElementById('{!$Component.theForm.retired}').checked == true){
            $("#resignorRetireddialog").dialog("open");
            return false;
        }else if(document.getElementById('{!$Component.theForm.resigned}').checked
                 && $("[id$=btnCheckbox4]").is(':checked') == false){
            document.getElementById('{!$Component.theForm.resignedProvider_refer}').checked = false;
            $("#resignorRetireddialog").dialog("open");
            return false;
        }else{
            saveProviderData();
        }
    }
