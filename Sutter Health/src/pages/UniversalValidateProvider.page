                                            <apex:outputText rendered="{!if(providerSearchFlag == true, true, false)}">
                                                <li id="li2"><a href="#tab2"><i class="fa fa-user"></i></a></li>
                                                <li id="li3"><a href="#tab3"><i class="fa fa-users"></i></a></li>
                                            </apex:outputText>
                                            <li id="li4"><a href="#tab4"><i class="fa fa-location-arrow"></i></a></li>
                                            <apex:outputText rendered="{!if(providerSearchFlag == true, true, false)}">
                                                <li id="li5"><a href="#tab5"><i class="fa fa-globe"></i></a></li>
                                            </apex:outputText>
                                            
                                        </ul>
                                        <div class="tab-content">
                                            <div id="tab1" class="tab-pane">
                                                
