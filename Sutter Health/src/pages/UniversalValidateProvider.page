                                        <ul class="nav nav-tabs" id="ul-menu-list">
                                            <li class="active" id="li1"><a href="#tab1">Provider Details</a></li>
                                            <apex:outputText rendered="{!if(providerSearchFlag == true, true, false)}">
                                                <li id="li2"><a href="#tab2">General Info</a></li>
                                                <li id="li3"><a href="#tab3">Other Details</a></li>
                                            </apex:outputText>
                                            <li id="li4"><a href="#tab4">Clinical Address Information</a></li>
                                            <apex:outputText rendered="{!if(providerSearchFlag == true, true, false)}">
                                                <li id="li5"><a href="#tab5">Patient Facing Address Information</a></li>
                                            </apex:outputText>
                                            
