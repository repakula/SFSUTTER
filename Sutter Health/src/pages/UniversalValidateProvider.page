<apex:page id="pageId" controller="UniversalProviderController" showHeader="false" standardStylesheets="false" sidebar="false" action="{!ifUserAuthenticated}">
    <head>
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
        <meta name="description" content="Accenture"/>
        <c:PDI_Portalbootstrap_resources ></c:PDI_Portalbootstrap_resources>
        <c:PDI_portalMenuCSS ></c:PDI_portalMenuCSS>
        <apex:stylesheet value="https://code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css" id="jqueryCss"/>
        <apex:includeScript value="https://code.jquery.com/ui/1.11.4/jquery-ui.js" id="js"/>
        <apex:includeScript value="{!URLFOR($Resource.PDI_bootstrap_checkbox, 'dist/js/bootstrap-checkbox.min.js')}" id="checkboxJS"/>
        
        <apex:includeScript value="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js" />
        <apex:stylesheet value="https://cdn.datatables.net/1.10.9/css/jquery.dataTables.min.css" />
        
        <apex:includeScript value="https://cdn.datatables.net/rowreorder/1.1.2/js/dataTables.rowReorder.min.js"/>
        <apex:includeScript value="https://cdn.datatables.net/responsive/2.1.0/js/dataTables.responsive.min.js"/>
        <apex:stylesheet value="https://cdn.datatables.net/rowreorder/1.1.2/css/rowReorder.dataTables.min.css"/>
        <apex:stylesheet value="https://cdn.datatables.net/responsive/2.1.0/css/responsive.dataTables.min.css"/>
    </head>
    
    <body>
        <div id="container"><!--Body-Container-->
            <div class="hidden-header"></div>
            <header class="clearfix">
                <c:PDI_PortalHeader ></c:PDI_PortalHeader>    
                
                <div class="navbar navbar-inverse" role="navigation" id="slide-nav">
                    <div class="container">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>                        
                            </button>
                            <a class="navbar-brand" href="#">
                                <apex:image url="{!URLFOR($Resource.logo)}" width="162" height="54" id="logo"/>
                            </a>
                        </div><!--navbar--header-->
                        <div id="slidemenu">
                            <ul class="nav navbar-nav">
                                <li><a href="javascript:redirectToHome();">Home</a></li>
                                <apex:outputText rendered="{!if($Api.Session_ID != null && $User.LastName != 'Site Guest User',true,false)}" id="profiletxt">
                                    <li><a href="UniversalValidateProvider" class="active">Profile</a></li>
                                </apex:outputText>
                                <li><a href="javascript:redirectToFAQ();">FAQ</a></li>
                            </ul>
                        </div><!--slidemenu-->
                    </div><!-- Nav Container-->
                </div><!--Nav bar-->
            </header>
            
            <apex:outputpanel id="providerPanel">
                
                <apex:form id="theForm" styleclass="form-horizontal" style="padding:3%;">
                    <apex:inputHidden value="{!contactId}" id="myHiddenField"/>
                    
                    <apex:actionFunction action="{!redirectoHome}" name="redirectoHomePage" id="homeAction"/>
                    <apex:actionFunction action="{!redirectoFAQ}" name="redirectToFAQPage" id="faqAction"/>
                    <apex:actionFunction action="{!redirectoChangePassword}" name="redirectoChangePass" id="changePasswdAction"/>
                    <apex:actionFunction action="{!redirectoProviderList}" name="redirecttoProviderList" id="redirecttoProviderList"/>
                    <apex:actionFunction id="retrieveProviderProfile" action="{!retrieveProviderProfile}" name="redirectoProfile"/>
                <apex:outputText rendered="{!if(providerFlag == true, true, false)}">
                    <div class="container" id="home"><!--Profile Container-->
                        
                        <div style="display: none;">
                            <apex:inputCheckbox id="practicing" value="{!summaryObj.Practicing_Physician__c}" />
                            <apex:inputCheckbox id="resigned" value="{!summaryObj.Resigned_From_Sutter__c}" />
                            <apex:inputCheckbox id="retired" value="{!summaryObj.Retired_Departed__c}" />
                            <apex:inputCheckbox id="resignedProvider_refer" value="{!summaryObj.Is_Resigned_Provider_refer__c}" />
                            <apex:inputCheckbox id="providerAccessType" value="{!summaryObj.Provider_Access_Type__c}"/>
                            <apex:inputCheckbox id="providerFlag" value="{!providerFlag}"/>
                        </div>
                        
                        <apex:outputText rendered="{!if(providerInfo.lastModifiedDate != null && $Profile.Name == 'PDV Community Provider', true, false)}" id="modifyDate">
                            <font color="blue" size="2">Last Validated On : {!providerInfo.lastModifiedDate}</font>
                        </apex:outputText><!--last validate text-->
                        <apex:outputPanel id="formPanel">
                            <apex:outputText rendered="{!if(section == 'form' && summaryObj != null, true, false)}" id="profileFrm">    
                                <div><apex:outputText id="headerTxt">{!$Label.CL597_PDI_PortalHeaderTxt}</apex:outputText></div>
                                <span><div class="requiredInput"><div class="requiredBlock"></div></div>
                                    <font color="red"> | = Fields highlighted in red are required.</font></span>
                                
                                <div class="row">
                                    <div class="col-sm-6 ">
                                        <apex:outputPanel id="errorPanel">
                                            <apex:outputText >
                                                <font color="red" size="5"><apex:pageMessages /></font>
                                            </apex:outputText>
                                        </apex:outputPanel>
                                    </div>
                                </div>              
                                <div class="panel-group">
                                    <script>
                                    /* Active class is added to clicked Tab */
                                    $('ul.nav-tabs li a').click(function (e) {
                                        $('ul.nav-tabs li.active').removeClass('active')
                                        $(this).parent('li').addClass('active')
                                        
                                        $(this).parent('tab-pane fade').addClass('in active')
                                    })
                                    
                                    /* Div change on click of Tab */
                                    $("#ul-menu-list li").click(function () {
                                        $('.tab-pane').hide().eq($(this).index()).show();
                                    });
                                    /* Div change on click of Tab mobile */
                                    $("#ul-menu-mobile li").click(function () {
                                        $('.tab-pane').hide().eq($(this).index()).show();
                                    });
                                    
                                    /* button click next tab */
                                    $('.btnNext').click(function(){
                                        $('.nav-tabs > .active').next('li').trigger('click');
                                        $('.nav-tabs > .active').next('li').find('a').trigger('click');
                                        
                                    });
                                    
                                    /* button click previous tab */
                                    $('.btnPrevious').click(function(){
                                        $('.nav-tabs > .active').prev('li').trigger('click');
                                        $('.nav-tabs > .active').prev('li').find('a').trigger('click');
                                        
                                    });
                                    
                                    /*Prevent Scroll jump on tab change */
                                    $('.nav-tabs li a').click(function (e) {
                                        e.preventDefault();
                                        $(this).tab('show');
                                        $('.tab-content > .tab-pane.active').jScrollPane();
                                    });
                                    
                                    if(document.getElementById('{!$Component.theForm.practicing}').checked){
                                        $("#btnradio1").prop('checked', true);
                                        //document.getElementById("btnradio1").checked = true;
                                    }
                                    if(document.getElementById('{!$Component.theForm.resigned}').checked){
                                        document.getElementById("btnradio2").checked = true;
                                        document.getElementById("resignedReferDiv").style.display = 'block';
                                        if(document.getElementById('{!$Component.theForm.resignedProvider_refer}').checked){
                                            $("#btnCheckbox4").prop('checked', true);
                                            resignreferreddisableText(true);
                                        }
                                    }
                                    if(document.getElementById('{!$Component.theForm.retired}').checked){
                                        $("#btnradio3").prop('checked', true);
                                        //document.getElementById("btnradio3").checked = true;
                                    }
                                    $('#btnCheckbox4').checkboxpicker({
                                    });
                                    </script>
                                    <div class="panel panel-primary"><!--Provider Status section starts-->
